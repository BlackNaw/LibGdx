package ManyBalls;

import java.util.Random;

public enum Color {
	amarilla, azul, morada, naranja, roja, verde;
	public static String getRandomColor() {
		switch (new Random().nextInt(6)) {
		case 0:
			return "amarilla";
		case 1:
			return "azul";
			break;
		case 2:
			return "morada";
			break;
		case 3:
			return "naranja";
			break;
		case 4:

			break;
		case 5:

			break;
		default:
			break;
		}

	}
}
