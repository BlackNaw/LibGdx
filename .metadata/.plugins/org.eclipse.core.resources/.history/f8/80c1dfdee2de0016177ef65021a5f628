package com.mygdx.game;

public class Rectangulo {

	public Posicion posicion;
	public int ancho, alto;
	public int arriba=posicion.x+ancho, derecha=posicion.y+alto;
	
	
	public Rectangulo(Posicion posicion, int ancho, int alto, int arriba, int derecha) {
		super();
		this.posicion = posicion;
		this.ancho = ancho;
		this.alto = alto;
		this.arriba = arriba;
		this.derecha = derecha;
	}
	
	
	
}
